@model AHP2.Models.PartialMenuViewModels

@{ 
    
}

<table class="table table-hover well well-sm">
    @foreach(var e in Model.GetAllMenuItems)
    {
        <tr>
            <td>
                @if(e == Model.MenuItem)
                {
                    @Html.ActionLink(@Model.GetNameMenuItem(e), "Diverter", "PartialMenu"
              , new { id = (int)Model.MenuItem + "-" + Model.ObjectRoueting}, htmlAttributes: new object { })
                }
                else
                {
                    @Html.ActionLink(@Model.GetNameMenuItem(e), "Diverter", "PartialMenu"
               , routeValues: new int[] { (int)Model.MenuItem, Model.ObjectRoueting }, htmlAttributes: new object { })
                }

            </td>
        </tr>
    }
</table>